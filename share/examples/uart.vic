PIC P16F690;

Main {
    setup UART, 9600; # set up USART for transmit
    write UART, "Hello World!";
    #write UART, "100";
    #write UART, 100;
    #write UART, 0x12345678;
    #$ss = "Hello World!";
    #write UART, $ss;
}

Simulator {
    attach UART, 9600;
    stop_after 2s;
    log UART;
    scope UART;
}
